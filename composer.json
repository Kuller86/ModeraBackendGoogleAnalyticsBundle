{
    "name": "modera/backend-google-analytics-bundle",
    "type": "symfony-bundle",
    "description": "Provides support for gathering 'pageview' analytics in backend using Google Analytics.",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergei Lissovski",
            "email": "sergei.lissovski@modera.org",
            "homepage": "http://sergei.lissovski.org/"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/framework-bundle": "~2.0",
        "modera/config-bundle": "~1.0",
        "modera/foundation-bundle": "~1.0",
        "sergeil/expander-bundle": "~1.0",
        "modera/security-bundle": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "phake/phake": "~2.0"
    },
    "autoload": {
        "psr-0": { "Modera\\BackendGoogleAnalyticsBundle": "" }
    },
    "target-dir": "Modera/BackendGoogleAnalyticsBundle",
    "extra": {
        "modera-module": {
            "register-bundle": "Modera\\BackendGoogleAnalyticsBundle\\ModeraBackendGoogleAnalyticsBundle"
        },
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}